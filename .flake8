[flake8]
exclude =
    .git,
    __pycache__,
    build,
    builds,
    env,
    github_repo,
    .pytest_cache,
    sphinx,
    sphinx/source/*.py
    sphinx/build/*.py,
    sphinx/temp/*.py,
    .eggs/*.py,
    versioneer.py,
max-line-length=88
select = C,E,F,W,B,B950
extend-ignore = E203, E501, E722, F401, F403, W503
